	REGION 6 - Right station
	    
	    IF #iUDT_Configuration.GlobalConfig.StationVariant = 1 OR #iUDT_Configuration.GlobalConfig.StationVariant = 3 THEN
	        
	        REGION 6.1 - Indexing 
	            
	            // Start indexing after request if there are empty stations and no pushout is active
	            IF #sx_RightActivateIndex AND #si_RightEmptyStations > 0
	                AND NOT (#sx_RightPushOutActiveStation1
	                OR #sx_RightPushOutActiveStation2
	                OR #sx_RightPushOutActiveStation3
	                OR #sx_RightPushOutActiveStation4
	                OR #sx_RightPushOutActiveStation5)
	            THEN
	                
	                #sx_RightIndexing := TRUE;
	                
	            END_IF;
	            
	            
	            IF NOT #sx_RightIndexing THEN                                      // If there is no indexing in progress
	                
	                #si_RightDestinationD541 := 0;                                // Reset RAT destination
	                #sx_RightExternalRequestHold550 := TRUE;                      // Hold station conveyors
	                #sx_RightExternalRequestHold560 := TRUE;
	                #sx_RightExternalRequestHold570 := TRUE;
	                #sx_RightExternalRequestHold580 := TRUE;
	                #sx_RightExternalRequestHold590 := TRUE;
	                
	            ELSE
	                
	                IF #sR_TRIG_PEC540.Q AND #si_RightEmptyStations > 0 THEN       //Count down empty stations after diverting a tote into station
	                    #si_RightEmptyStations -= 1;
	                END_IF;
	                
	                #si_RightDestinationD541 := 4;                               // Set RAT destination
	                #sx_RightExternalRequestHold550 := FALSE;                    // Release hold of station conveyors
	                #sx_RightExternalRequestHold560 := FALSE;
	                #sx_RightExternalRequestHold570 := FALSE;
	                #sx_RightExternalRequestHold580 := FALSE;
	                #sx_RightExternalRequestHold590 := FALSE;
	                
	                IF                                                           //End indexing when all totes are on position
	                    NOT #iqUDT_WorkstationStatus."550".PEC_EoS.PEC
	                    AND NOT #iqUDT_WorkstationStatus."560".PEC_EoS.PEC
	                    AND NOT #iqUDT_WorkstationStatus."570".PEC_EoS.PEC
	                    AND NOT #iqUDT_WorkstationStatus."580".PEC_EoS.PEC
	                    AND NOT #iqUDT_WorkstationStatus."590".PEC_EoS.PEC
	                    AND NOT (#sUDT_Interface550.ActualSpeed > 0) THEN
	                    
	                    #sx_RightIndexing := FALSE;
	                    
	                END_IF;
	            END_IF;
	            
	        END_REGION
	        
	        REGION 6.2 - Push Out
	            
	            IF #iUDT_Configuration.GlobalConfig.RightCubbyNumbering = 1 THEN
	                #sa_RightCubby[1] := 10;
	                #sa_RightCubby[2] := 9;
	                #sa_RightCubby[3] := 8;
	                #sa_RightCubby[4] := 7;
	                #sa_RightCubby[5] := 6;
	            ELSIF #iUDT_Configuration.GlobalConfig.RightCubbyNumbering = 2 THEN
	                #sa_RightCubby[1] := 5;
	                #sa_RightCubby[2] := 4;
	                #sa_RightCubby[3] := 3;
	                #sa_RightCubby[4] := 2;
	                #sa_RightCubby[5] := 1;
	            END_IF;
	            
	            
	            REGION 6.2.0 - Pushout conveyors hold
	                IF #sFB_TimerOnOffDelay_Hold710.qx_DelayedSignal THEN
	                    #sx_RightExternalRequestHold710 := TRUE;
	                END_IF;
	                
	                IF #sFB_TimerOnOffDelay_Hold720.qx_DelayedSignal THEN
	                    #sx_RightExternalRequestHold720 := TRUE;
	                END_IF;
	                
	                IF #sFB_TimerOnOffDelay_Hold730.qx_DelayedSignal THEN
	                    #sx_RightExternalRequestHold730 := TRUE;
	                END_IF;
	                
	                IF #sFB_TimerOnOffDelay_Hold740.qx_DelayedSignal THEN
	                    #sx_RightExternalRequestHold740 := TRUE;
	                END_IF;
	                
	                IF #sFB_TimerOnOffDelay_Hold750.qx_DelayedSignal THEN
	                    #sx_RightExternalRequestHold750 := TRUE;
	                END_IF;
	            END_REGION
	            
	            REGION 6.2.1 - Push Out Station 1
	                
	                // Start automatic pushout after request if there is no indexing procedure and a tote is present on first station
	                IF #iqUDT_RightWESInterface.Field[#sa_RightCubby[1]].WES2PLC.RemoveContainer AND NOT #sx_RightIndexing
	                    AND NOT #iqUDT_WorkstationStatus."590".PEC_EoS.PEC THEN
	                    
	                    #sx_RightPushOutActiveStation1 := TRUE;
	                    #iqUDT_RightWESInterface.Field[#sa_RightCubby[1]].WES2PLC.RemoveContainer := FALSE;
	                END_IF;
	                
	                IF #sx_RightPushOutActiveStation1 THEN                         //release hold
	                    #sx_RightExternalRequestHold590 := FALSE;
	                    
	                    IF #sFB_RightAngledTransfer611.qx_ExternalSendRequestB                   //after tote is already on RAT, hold the conveyor and end pushout procedure
	                        AND #iUDT_Configuration.GlobalConfig.StationVariant <> 3             // and station has not only right station  
	                    THEN
	                        #sx_RightExternalRequestHold590 := TRUE;
	                        #sx_RightPushOutActiveStation1 := FALSE;
	                    END_IF;
	                    
	                    IF #sFB_RightAngledTransfer211.qx_ExternalSendRequestB                   //after tote is already on RAT, hold the conveyor and end pushout procedure
	                        AND #iUDT_Configuration.GlobalConfig.StationVariant = 3             // and station has only right station  
	                    THEN
	                        #sx_RightExternalRequestHold590 := TRUE;
	                        #sx_RightPushOutActiveStation1 := FALSE;
	                    END_IF;
	                    
	                END_IF;
	                
	            END_REGION
	            
	            REGION 6.2.2 - Push Out station 2
	                
	                IF #iqUDT_RightWESInterface.Field[#sa_RightCubby[2]].WES2PLC.RemoveContainer THEN        //set pushout to active after request
	                    #sx_RightPushOutActiveStation2 := TRUE;
	                    #iqUDT_RightWESInterface.Field[#sa_RightCubby[2]].WES2PLC.RemoveContainer := FALSE;
	                END_IF;
	                
	                IF #sx_RightPushOutActiveStation2                               // After station is empty and tote has been detected on pushout conveyor release holds
	                    AND #iqUDT_WorkstationStatus."580".PEC_EoS.PEC
	                    AND #sFB_TimerOnOffDelay_Detect740.qx_DelayedSignal THEN
	                    
	                    #sx_RightExternalRequestHold740 := FALSE;
	                    #sx_RightExternalRequestHold750 := FALSE;
	                    
	                    FILL_BLK(IN := 0,                                             //clear tracking
	                             COUNT := REAL_TO_ULINT(DINT_TO_REAL(#iUDT_Configuration."580".Conveyor.Common.Length) * #cr_ClearTrackingModifier),
	                             OUT => #sa_TrackingArray580[0]);
	                    
	                    #sx_RightPushOutActiveStation2 := FALSE;                       //end pushout procedure
	                    
	                END_IF;
	                
	                
	            END_REGION
	            
	            REGION 6.2.3 - Push Out station 3
	                
	                IF #iqUDT_RightWESInterface.Field[#sa_RightCubby[3]].WES2PLC.RemoveContainer THEN          //set pushout to active after request
	                    #sx_RightPushOutActiveStation3 := TRUE;
	                    #iqUDT_RightWESInterface.Field[#sa_RightCubby[3]].WES2PLC.RemoveContainer := FALSE;
	                END_IF;
	                
	                IF #sx_RightPushOutActiveStation3
	                    AND #iqUDT_WorkstationStatus."570".PEC_EoS.PEC
	                    AND #sFB_TimerOnOffDelay_Detect730.qx_DelayedSignal THEN                // After station is empty and tote has been detected on pushout conveyor release holds
	                    
	                    #sx_RightExternalRequestHold730 := FALSE;
	                    #sx_RightExternalRequestHold740 := FALSE;
	                    #sx_RightExternalRequestHold750 := FALSE;
	                    
	                    FILL_BLK(IN := 0,                                                         //clear tracking
	                             COUNT := REAL_TO_ULINT(DINT_TO_REAL(#iUDT_Configuration."570".Conveyor.Common.Length) * #cr_ClearTrackingModifier),
	                             OUT => #sa_TrackingArray570[0]);
	                    
	                    #sx_RightPushOutActiveStation3 := FALSE;                              //end pushout procedure
	                    
	                END_IF;
	            END_REGION
	            
	            REGION 6.2.4 - Push Out station 4
	                
	                IF #iqUDT_RightWESInterface.Field[#sa_RightCubby[4]].WES2PLC.RemoveContainer THEN              //set pushout to active after request
	                    #sx_RightPushOutActiveStation4 := TRUE;
	                    #iqUDT_RightWESInterface.Field[#sa_RightCubby[4]].WES2PLC.RemoveContainer := FALSE;
	                END_IF;
	                
	                IF #sx_RightPushOutActiveStation4
	                    AND #iqUDT_WorkstationStatus."560".PEC_EoS.PEC
	                    AND #sFB_TimerOnOffDelay_Detect720.qx_DelayedSignal THEN                // After station is empty and tote has been detected on pushout conveyor release holds
	                    
	                    #sx_RightExternalRequestHold720 := FALSE;
	                    #sx_RightExternalRequestHold730 := FALSE;
	                    #sx_RightExternalRequestHold740 := FALSE;
	                    #sx_RightExternalRequestHold750 := FALSE;
	                    
	                    FILL_BLK(IN := 0,                                                      //clear tracking
	                             COUNT := REAL_TO_ULINT(DINT_TO_REAL(#iUDT_Configuration."560".Conveyor.Common.Length) * #cr_ClearTrackingModifier),
	                             OUT => #sa_TrackingArray560[0]);
	                    
	                    #sx_RightPushOutActiveStation4 := FALSE;                          //end pushout procedure
	                    
	                END_IF;
	                
	            END_REGION
	            
	            REGION 6.2.5 - Push Out station 5
	                
	                IF #iqUDT_RightWESInterface.Field[#sa_RightCubby[5]].WES2PLC.RemoveContainer THEN          //set pushout to active after request
	                    #sx_RightPushOutActiveStation5 := TRUE;
	                    #iqUDT_RightWESInterface.Field[#sa_RightCubby[5]].WES2PLC.RemoveContainer := FALSE;
	                END_IF;
	                
	                IF #sx_RightPushOutActiveStation5
	                    AND #iqUDT_WorkstationStatus."550".PEC_EoS.PEC
	                    AND #sFB_TimerOnOffDelay_Detect710.qx_DelayedSignal THEN                // After station is empty and tote has been detected on pushout conveyor release holds
	                    
	                    #sx_RightExternalRequestHold710 := FALSE;
	                    #sx_RightExternalRequestHold720 := FALSE;
	                    #sx_RightExternalRequestHold730 := FALSE;
	                    #sx_RightExternalRequestHold740 := FALSE;
	                    #sx_RightExternalRequestHold750 := FALSE;
	                    
	                    FILL_BLK(IN := 0,                                            //clear tracking
	                             COUNT := REAL_TO_ULINT(DINT_TO_REAL(#iUDT_Configuration."550".Conveyor.Common.Length) * #cr_ClearTrackingModifier),
	                             OUT => #sa_TrackingArray550[0]);
	                    
	                    #sx_RightPushOutActiveStation5 := FALSE;                      //end pushout procedure
	                END_IF;
	                
	            END_REGION
	            
	            REGION 6.2.6 - Pushout support logic
	                
	                IF #iUDT_Configuration.GlobalConfig.PushoutsPresent AND
	                    ((#iUDT_Configuration.GlobalConfig.StationVariant = 1 OR
	                    #iUDT_Configuration.GlobalConfig.StationVariant = 3)) THEN
	                    
	                    IF (NOT #iqUDT_WorkstationStatus."550".PEC_EoS.PEC AND #sx_RightPushOutActiveStation5) OR
	                        (NOT #iqUDT_WorkstationStatus."560".PEC_EoS.PEC AND #sx_RightPushOutActiveStation4) THEN
	                        #sx_Hold760 := FALSE;
	                        
	                    ELSIF NOT (#sx_LeftPushOutActiveStation5 OR #sx_LeftPushOutActiveStation4) THEN
	                        #sx_Hold760 := TRUE; //Hold pushout conveyors
	                    END_IF;
	                    
	                    //Package present on the right side pecs, pushouts run  
	                    IF (NOT #iqUDT_WorkstationStatus."570".PEC_EoS.PEC AND #sx_RightPushOutActiveStation3) OR
	                        (NOT #iqUDT_WorkstationStatus."580".PEC_EoS.PEC AND #sx_RightPushOutActiveStation2) THEN
	                        #sx_Hold770 := FALSE;
	                        
	                    ELSIF NOT (#sx_LeftPushOutActiveStation3 OR #sx_LeftPushOutActiveStation2) THEN
	                        #sx_Hold770 := TRUE; //Hold pushout conveyors
	                    END_IF;
	                END_IF;
	                
	            END_REGION
	        END_REGION
	        
	        REGION 6.3 WES Communication
	            
	            REGION 6.3.1 Barcode to WES
	                IF NOT #sx_RightIndexing
	                THEN
	                    
	                    REGION 590
	                        // first position
	                        // 
	                        IF NOT #sx_RightPushOutActiveStation1
	                            AND NOT #iqUDT_WorkstationStatus."590".PEC_EoS.PEC
	                            AND #sa_TrackingArray590[(#iUDT_Configuration."590".PEC_Eos.Distance / 10) - 1] > 0
	                        THEN
	                            
	                            IF #iqUDT_RightWESInterface.Field[#sa_RightCubby[1]].WES2PLC.SetContainerID THEN
	                                
	                                "DB_GIN".GIN[#sa_TrackingArray590[(#iUDT_Configuration."590".PEC_Eos.Distance / 10) - 1]].General.Identification.Barcode := #iqUDT_RightWESInterface.Field[#sa_RightCubby[1]].WES2PLC.ContainerID;
	                                #iqUDT_RightWESInterface.Field[#sa_RightCubby[1]].WES2PLC.SetContainerID := FALSE;
	                                
	                            END_IF;
	                            
	                            IF #iqUDT_RightWESInterface.Field[#sa_RightCubby[1]].PLC2WES.ContainerID <> "DB_GIN".GIN[#sa_TrackingArray590[(#iUDT_Configuration."590".PEC_Eos.Distance / 10) - 1]].General.Identification.Barcode
	                            THEN
	                                
	                                #iqUDT_RightWESInterface.Field[#sa_RightCubby[1]].PLC2WES.ContainerID := "DB_GIN".GIN[#sa_TrackingArray590[(#iUDT_Configuration."590".PEC_Eos.Distance / 10) - 1]].General.Identification.Barcode;
	                                #iqUDT_RightWESInterface.Field[#sa_RightCubby[1]].PLC2WES.SetContainerID := TRUE;
	                            END_IF;
	                            
	                        ELSE
	                            IF #iqUDT_RightWESInterface.Field[#sa_RightCubby[1]].PLC2WES.ContainerID <> #cs_MissingBarcodeCode
	                            THEN
	                                #iqUDT_RightWESInterface.Field[#sa_RightCubby[1]].PLC2WES.ContainerID := #cs_MissingBarcodeCode;
	                                #iqUDT_RightWESInterface.Field[#sa_RightCubby[1]].PLC2WES.SetContainerID := TRUE;
	                                
	                            END_IF;
	                            
	                        END_IF;
	                    END_REGION ;
	                    REGION 580
	                        // second position
	                        // 
	                        IF NOT #iqUDT_WorkstationStatus."580".PEC_EoS.PEC
	                            AND #sa_TrackingArray580[(#iUDT_Configuration."580".PEC_Eos.Distance / 10) - 1] > 0
	                        THEN
	                            
	                            IF #iqUDT_RightWESInterface.Field[#sa_RightCubby[2]].WES2PLC.SetContainerID THEN
	                                
	                                "DB_GIN".GIN[#sa_TrackingArray580[(#iUDT_Configuration."580".PEC_Eos.Distance / 10) - 1]].General.Identification.Barcode := #iqUDT_RightWESInterface.Field[#sa_RightCubby[2]].WES2PLC.ContainerID;
	                                #iqUDT_RightWESInterface.Field[#sa_RightCubby[2]].WES2PLC.SetContainerID := FALSE;
	                                
	                            END_IF;
	                            
	                            IF #iqUDT_RightWESInterface.Field[#sa_RightCubby[2]].PLC2WES.ContainerID <> "DB_GIN".GIN[#sa_TrackingArray580[(#iUDT_Configuration."580".PEC_Eos.Distance / 10) - 1]].General.Identification.Barcode
	                            THEN
	                                
	                                #iqUDT_RightWESInterface.Field[#sa_RightCubby[2]].PLC2WES.ContainerID := "DB_GIN".GIN[#sa_TrackingArray580[(#iUDT_Configuration."580".PEC_Eos.Distance / 10) - 1]].General.Identification.Barcode;
	                                #iqUDT_RightWESInterface.Field[#sa_RightCubby[2]].PLC2WES.SetContainerID := TRUE;
	                            END_IF;
	                            
	                        ELSE
	                            IF #iqUDT_RightWESInterface.Field[#sa_RightCubby[2]].PLC2WES.ContainerID <> #cs_MissingBarcodeCode
	                            THEN
	                                #iqUDT_RightWESInterface.Field[#sa_RightCubby[2]].PLC2WES.ContainerID := #cs_MissingBarcodeCode;
	                                #iqUDT_RightWESInterface.Field[#sa_RightCubby[2]].PLC2WES.SetContainerID := TRUE;
	                                
	                            END_IF;
	                            
	                        END_IF;
	                    END_REGION ;
	                    REGION 570
