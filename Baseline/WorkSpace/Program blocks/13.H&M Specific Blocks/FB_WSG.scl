	11/02/2025  | 2.5       | K.Budzyn      | Redefined HardwareOk structure in block, created and developed pushouts and lift gate logic.
	*)
	
	
	//
	//Station variant // 1 -> Left and Right exists // 2 -> Left only // 3 -> Right only 
	//
	
	
	
	
	
	REGION 0 - Initial Left Station
	    
	    IF #iUDT_Configuration.GlobalConfig.StationVariant = 3 THEN
	        
	        #iqUDT_LeftWESInterface := #iqUDT_RightWESInterface;
	        
	        #iqUDT_WorkstationStatus."550".PEC_EoS.PEC := #iqUDT_WorkstationStatus."150".PEC_EoS.PEC;
	        #iqUDT_WorkstationStatus."560".PEC_EoS.PEC := #iqUDT_WorkstationStatus."160".PEC_EoS.PEC;
	        #iqUDT_WorkstationStatus."570".PEC_EoS.PEC := #iqUDT_WorkstationStatus."170".PEC_EoS.PEC;
	        #iqUDT_WorkstationStatus."580".PEC_EoS.PEC := #iqUDT_WorkstationStatus."180".PEC_EoS.PEC;
	        #iqUDT_WorkstationStatus."590".PEC_EoS.PEC := #iqUDT_WorkstationStatus."190".PEC_EoS.PEC;
	        #iqUDT_WorkstationStatus."710".PEC_EoS.PEC := #iqUDT_WorkstationStatus."310".PEC_EoS.PEC;
	        #iqUDT_WorkstationStatus."720".PEC_EoS.PEC := #iqUDT_WorkstationStatus."320".PEC_EoS.PEC;
	        #iqUDT_WorkstationStatus."730".PEC_EoS.PEC := #iqUDT_WorkstationStatus."330".PEC_EoS.PEC;
	        #iqUDT_WorkstationStatus."740".PEC_EoS.PEC := #iqUDT_WorkstationStatus."340".PEC_EoS.PEC;
	        #iqUDT_WorkstationStatus."750".PEC_EoS.PEC := #iqUDT_WorkstationStatus."350".PEC_EoS.PEC;
	        #iqUDT_WorkstationStatus."520".PEC_EoS.PEC := #iqUDT_WorkstationStatus."110".PEC_EoS.PEC;
	        #iqUDT_WorkstationStatus."530".PEC_EoS.PEC := #iqUDT_WorkstationStatus."130".PEC_EoS.PEC;
	        #iqUDT_WorkstationStatus."540".PEC_EoS.PEC := #iqUDT_WorkstationStatus."140".PEC_EoS.PEC;
	        #iqUDT_WorkstationStatus."610".PEC_EoS.PEC := #iqUDT_WorkstationStatus."210".PEC_EoS.PEC;
	        #iqUDT_WorkstationStatus."620".PEC_EoS.PEC := #iqUDT_WorkstationStatus."220".PEC_EoS.PEC;
	        
	    END_IF;
	    
	    
	    IF #iUDT_Configuration.GlobalConfig.StationVariant > 0 THEN
	        
	        REGION 0.1 - Reset triggers
	            
	            IF #sx_ActivationLeft
	            THEN
	                #iqUDT_LeftWESInterface.WES2PLC.On := FALSE;
	                #sx_ActivationLeft := FALSE;
	            END_IF;
	            
	            IF #sx_DeactivationLeft
	            THEN
	                #iqUDT_LeftWESInterface.WES2PLC.Off := FALSE;
	                #sx_DeactivationLeft := FALSE;
	            END_IF;
	            
	        END_REGION
	        
	        REGION 0.2 - Triggers
	            
	            #sR_TRIG_PEC140(CLK := #iqUDT_WorkstationStatus."140".PEC_EoS.PEC);    //Rising Edge PEC on conveyor 140 for indexing into station count
	            
	            #sR_TRIG_B111(CLK := #ix_MaintenanceRequestStart1 OR #ix_MaintenanceRequestStart2);      //Rising edge detection for liftgate start button
	            
	            #sR_TRIG_B112(CLK := #ix_MaintenanceRequestStop1 OR #ix_MaintenanceRequestStop2);          //Rising edge detection for liftgate start button
	            
	            
	            #sR_TRIG_ActivateLeftStation(CLK := #iqUDT_LeftWESInterface.WES2PLC.On,     //Rising Edge activate left station
	                                         Q => #sx_ActivationLeft);
	            
	            #sR_TRIG_DeactivateLeftStation(CLK := #iqUDT_LeftWESInterface.WES2PLC.Off,   //Rising Edge deactivate left station
	                                           Q => #sx_DeactivationLeft);
	            
	        END_REGION
	        
	        REGION 0.3 - Counters
	            REGION 0.3.1 - Count empty stations 
	                
	                //If there is no indexing procedure then count empty stations
	                
	                IF NOT #sx_LeftIndexing THEN
	                    
	                    #sFB_CountActiveInputs_LeftStation(ix_Input1 := #iqUDT_WorkstationStatus."150".PEC_EoS.PEC,
	                                                       ix_Input2 := #iqUDT_WorkstationStatus."160".PEC_EoS.PEC,
	                                                       ix_Input3 := #iqUDT_WorkstationStatus."170".PEC_EoS.PEC,
	                                                       ix_Input4 := #iqUDT_WorkstationStatus."180".PEC_EoS.PEC,
	                                                       ix_Input5 := #iqUDT_WorkstationStatus."190".PEC_EoS.PEC,
	                                                       qi_ActiveInputsCount => #si_LeftEmptyStations);
	                END_IF;
	            END_REGION
	            
	            REGION 0.3.2 - Count free spaces on left buffer
	                
	                // If there's no maintenance performed on lift, then count free spaces, if there is - set them to 0, cause we don't want to divert anyway
	                
	                IF NOT #sx_MaintenanceIsPerformed AND #sx_LeftStationActive
	                THEN
	                    #sFB_CountActiveInputs_LeftBuffer(ix_Input1 := #iqUDT_WorkstationStatus."110".PEC_EoS.PEC,
	                                                      ix_Input2 := #iqUDT_WorkstationStatus."130".PEC_EoS.PEC,
	                                                      ix_Input3 := #iqUDT_WorkstationStatus."140".PEC_EoS.PEC);
	                ELSIF #sx_MaintenanceIsPerformed OR NOT #sx_LeftStationActive
	                THEN
	                    #sFB_CountActiveInputs_LeftBuffer.qi_ActiveInputsCount := 0;
	                END_IF;
	            END_REGION
	        END_REGION
	        
	        REGION 0.4 - Timers
	            
	            REGION 0.4.1 - Delay pushout conveyors hold signals after pushout started
	                
	                #sFB_TimerOnOffDelay_Hold350(iudi_OnDelayTime := #cudi_PushOutTime,
	                                             ix_SignalToDelay := NOT #sx_LeftExternalRequestHold350
	                                             AND #iqUDT_WorkstationStatus."310".PEC_EoS.PEC
	                                             AND #iqUDT_WorkstationStatus."320".PEC_EoS.PEC
	                                             AND #iqUDT_WorkstationStatus."330".PEC_EoS.PEC
	                                             AND #iqUDT_WorkstationStatus."340".PEC_EoS.PEC
	                                             AND #iqUDT_WorkstationStatus."350".PEC_EoS.PEC,
	                                             ix_Enable := TRUE);
	                
	                #sFB_TimerOnOffDelay_Hold340(iudi_OnDelayTime := #cudi_PushOutTime,
	                                             ix_SignalToDelay := NOT #sx_LeftExternalRequestHold340
	                                             AND #iqUDT_WorkstationStatus."310".PEC_EoS.PEC
	                                             AND #iqUDT_WorkstationStatus."320".PEC_EoS.PEC
	                                             AND #iqUDT_WorkstationStatus."330".PEC_EoS.PEC
	                                             AND #iqUDT_WorkstationStatus."340".PEC_EoS.PEC,
	                                             ix_Enable := TRUE);
	                
	                #sFB_TimerOnOffDelay_Hold330(iudi_OnDelayTime := #cudi_PushOutTime,
	                                             ix_SignalToDelay := NOT #sx_LeftExternalRequestHold330
	                                             AND #iqUDT_WorkstationStatus."310".PEC_EoS.PEC
	                                             AND #iqUDT_WorkstationStatus."320".PEC_EoS.PEC
	                                             AND #iqUDT_WorkstationStatus."330".PEC_EoS.PEC,
	                                             ix_Enable := TRUE);
	                
	                #sFB_TimerOnOffDelay_Hold320(iudi_OnDelayTime := #cudi_PushOutTime,
	                                             ix_SignalToDelay := NOT #sx_LeftExternalRequestHold320
	                                             AND #iqUDT_WorkstationStatus."310".PEC_EoS.PEC
	                                             AND #iqUDT_WorkstationStatus."320".PEC_EoS.PEC,
	                                             ix_Enable := TRUE);
	                
	                #sFB_TimerOnOffDelay_Hold310(iudi_OnDelayTime := #cudi_PushOutTime,
	                                             ix_SignalToDelay := NOT #sx_LeftExternalRequestHold310
	                                             AND #iqUDT_WorkstationStatus."310".PEC_EoS.PEC,
	                                             ix_Enable := TRUE);
	                
	                
	                
	            END_REGION
	            
	            REGION 0.4.2 - Delayed start of pushout conveyors after pushout detection
	                #sFB_TimerOnOffDelay_Detect340(iudi_OnDelayTime := #cudi_DetectTime,
	                                               ix_SignalToDelay := NOT #iqUDT_WorkstationStatus."340".PEC_EoS.PEC,
	                                               ix_Enable := TRUE);
	                
	                #sFB_TimerOnOffDelay_Detect330(iudi_OnDelayTime := #cudi_DetectTime,
	                                               ix_SignalToDelay := NOT #iqUDT_WorkstationStatus."330".PEC_EoS.PEC,
	                                               ix_Enable := TRUE);
	                
	                #sFB_TimerOnOffDelay_Detect320(iudi_OnDelayTime := #cudi_DetectTime,
	                                               ix_SignalToDelay := NOT #iqUDT_WorkstationStatus."320".PEC_EoS.PEC,
	                                               ix_Enable := TRUE);
	                
	                #sFB_TimerOnOffDelay_Detect310(iudi_OnDelayTime := #cudi_DetectTime,
	                                               ix_SignalToDelay := NOT #iqUDT_WorkstationStatus."310".PEC_EoS.PEC,
	                                               ix_Enable := TRUE);
	            END_REGION
	            
	            REGION 0.4.3 - Gate conveyor proximity sensor filtered signal
	                
	                #sFB_TimerOnOffDelay_GateIsDown111Filtered(iudi_OnDelayTime := #cudi_GateIsDownFilterTime,
	                                                           iudi_OffDelayTime := 10,
	                                                           ix_SignalToDelay := #ix_GateIsDown111,
	                                                           qx_DelayedSignal => #sx_GateIsDown111Filtered);
	            END_REGION
	        END_REGION
	        
	        REGION 0.5 - Left station activation
	            
	            IF #sx_ActivationLeft
	            THEN
	                #sx_LeftStationActive := TRUE;
	                #iqUDT_LeftWESInterface.PLC2WES.On := TRUE;
	            END_IF;
	            
	            IF #sx_DeactivationLeft
	            THEN
	                #sx_LeftStationActive := FALSE;
	                #iqUDT_LeftWESInterface.PLC2WES.Off := TRUE;
	            END_IF;
	            
	            
	        END_REGION
	        
	    END_IF;
	    
	END_REGION
	
	
	
	
	REGION 1 - Initial Right station
	    
	    IF #iUDT_Configuration.GlobalConfig.StationVariant = 1 OR #iUDT_Configuration.GlobalConfig.StationVariant = 3 THEN
	        
	        REGION 1.1 - Reset triggers
	            
	            IF #sx_ActivationRight THEN
	                #iqUDT_RightWESInterface.WES2PLC.On := FALSE;
	                #sx_ActivationRight := FALSE;
	            END_IF;
	            
	            IF #sx_DeactivationRight THEN
	                #iqUDT_RightWESInterface.WES2PLC.Off := FALSE;
	                #sx_DeactivationRight := FALSE;
	            END_IF;
	            
	        END_REGION
	        
	        REGION 1.2 - Triggers
	            
	            #sR_TRIG_PEC540(CLK := #iqUDT_WorkstationStatus."540".PEC_EoS.PEC);   //Rising Edge PEC on conveyor 540 for indexing into station count
	            
	            #sR_TRIG_ActivateRightStation(CLK := #iqUDT_RightWESInterface.WES2PLC.On,     //Rising Edge activate right station
	                                          Q => #sx_ActivationRight);
	            
	            #sR_TRIG_DeactivateRightStation(CLK := #iqUDT_RightWESInterface.WES2PLC.Off,   //Rising Edge deactivate right station
	                                            Q => #sx_DeactivationRight);
	            
	        END_REGION
	        
	        REGION 1.3 - Counters
	            
	            REGION 1.3.1 - Count empty stations on right side
	                
	                //If there is no indexing procedure then count empty stations
	                //
	                IF NOT #sx_RightIndexing THEN
	                    
	                    #sFB_CountActiveInputs_RightStation(ix_Input1 := #iqUDT_WorkstationStatus."550".PEC_EoS.PEC,
	                                                        ix_Input2 := #iqUDT_WorkstationStatus."560".PEC_EoS.PEC,
	                                                        ix_Input3 := #iqUDT_WorkstationStatus."570".PEC_EoS.PEC,
	                                                        ix_Input4 := #iqUDT_WorkstationStatus."580".PEC_EoS.PEC,
	                                                        ix_Input5 := #iqUDT_WorkstationStatus."590".PEC_EoS.PEC,
	                                                        qi_ActiveInputsCount => #si_RightEmptyStations);
	                END_IF;
	            END_REGION
	            
	            
	            
	            REGION 1.3.2 - Count free spaces on right buffer
	                
	                IF #sx_RightStationActive THEN
	                    #sFB_CountActiveInputs_RightBuffer(ix_Input1 := #iqUDT_WorkstationStatus."520".PEC_EoS.PEC,
	                                                       ix_Input2 := #iqUDT_WorkstationStatus."530".PEC_EoS.PEC,
	                                                       ix_Input3 := #iqUDT_WorkstationStatus."540".PEC_EoS.PEC);
	                ELSE
	                    #sFB_CountActiveInputs_RightBuffer.qi_ActiveInputsCount := 0;
	                END_IF;
	                
	            END_REGION
	            
	        END_REGION
	        
	        REGION 1.4 - Timers
	            
	            REGION 1.4.1 - Right station timers for holding pushout conveyors after pushout
	                
	                #sFB_TimerOnOffDelay_Hold750(iudi_OnDelayTime := #cudi_PushOutTime,
	                                             ix_SignalToDelay := NOT #sx_RightExternalRequestHold750
	                                             AND #iqUDT_WorkstationStatus."710".PEC_EoS.PEC
	                                             AND #iqUDT_WorkstationStatus."720".PEC_EoS.PEC
	                                             AND #iqUDT_WorkstationStatus."730".PEC_EoS.PEC
	                                             AND #iqUDT_WorkstationStatus."740".PEC_EoS.PEC
	                                             AND #iqUDT_WorkstationStatus."750".PEC_EoS.PEC,
	                                             ix_Enable := TRUE);
	                
	                #sFB_TimerOnOffDelay_Hold740(iudi_OnDelayTime := #cudi_PushOutTime,
	                                             ix_SignalToDelay := NOT #sx_RightExternalRequestHold740
	                                             AND #iqUDT_WorkstationStatus."710".PEC_EoS.PEC
	                                             AND #iqUDT_WorkstationStatus."720".PEC_EoS.PEC
	                                             AND #iqUDT_WorkstationStatus."730".PEC_EoS.PEC
	                                             AND #iqUDT_WorkstationStatus."740".PEC_EoS.PEC,
	                                             ix_Enable := TRUE);
	                
	                #sFB_TimerOnOffDelay_Hold730(iudi_OnDelayTime := #cudi_PushOutTime,
	                                             ix_SignalToDelay := NOT #sx_RightExternalRequestHold730
	                                             AND #iqUDT_WorkstationStatus."710".PEC_EoS.PEC
	                                             AND #iqUDT_WorkstationStatus."720".PEC_EoS.PEC
	                                             AND #iqUDT_WorkstationStatus."730".PEC_EoS.PEC,
	                                             ix_Enable := TRUE);
	                
	                #sFB_TimerOnOffDelay_Hold720(iudi_OnDelayTime := #cudi_PushOutTime,
	                                             ix_SignalToDelay := NOT #sx_RightExternalRequestHold720
	                                             AND #iqUDT_WorkstationStatus."710".PEC_EoS.PEC
	                                             AND #iqUDT_WorkstationStatus."720".PEC_EoS.PEC,
	                                             ix_Enable := TRUE);
	                
	                #sFB_TimerOnOffDelay_Hold710(iudi_OnDelayTime := #cudi_PushOutTime,
	                                             ix_SignalToDelay := NOT #sx_RightExternalRequestHold710
	                                             AND #iqUDT_WorkstationStatus."710".PEC_EoS.PEC,
	                                             ix_Enable := TRUE);
	            END_REGION
	            
	            REGION 1.4.2 - Right station delayed start of pushout conveyors
	                
	                #sFB_TimerOnOffDelay_Detect740(iudi_OnDelayTime := #cudi_DetectTime,
	                                               ix_SignalToDelay := NOT #iqUDT_WorkstationStatus."740".PEC_EoS.PEC,
	                                               ix_Enable := TRUE);
	                
	                #sFB_TimerOnOffDelay_Detect730(iudi_OnDelayTime := #cudi_DetectTime,
	                                               ix_SignalToDelay := NOT #iqUDT_WorkstationStatus."730".PEC_EoS.PEC,
	                                               ix_Enable := TRUE);
	                
	                #sFB_TimerOnOffDelay_Detect720(iudi_OnDelayTime := #cudi_DetectTime,
	                                               ix_SignalToDelay := NOT #iqUDT_WorkstationStatus."720".PEC_EoS.PEC,
	                                               ix_Enable := TRUE);
	                
	                #sFB_TimerOnOffDelay_Detect710(iudi_OnDelayTime := #cudi_DetectTime,
	                                               ix_SignalToDelay := NOT #iqUDT_WorkstationStatus."710".PEC_EoS.PEC,
	                                               ix_Enable := TRUE);
	                
	                #sFB_TimerGateTimeout(iudi_OnDelayTime := #cudi_GateTimout,
	                                      ix_SignalToDelay := NOT #sx_GateIsDown111Filtered,
	                                      qx_DelayedSignal => #sx_GateTimeout);
	                
	            END_REGION
	            
	        END_REGION END_IF;
	        
	        REGION 1.5 - Right station activation
	            
	            IF #sx_ActivationRight
	            THEN
	                #sx_RightStationActive := TRUE;
	                #iqUDT_RightWESInterface.PLC2WES.On := TRUE;
	            END_IF;
	            
	            IF #sx_DeactivationRight
	            THEN
	                #sx_RightStationActive := FALSE;
	                #iqUDT_RightWESInterface.PLC2WES.Off := TRUE;
	            END_IF;
	            
	        END_REGION
	        
	    END_IF;
	    
	END_REGION
	
	REGION 2 - Lift gate
	    
	    IF #iUDT_Configuration.GlobalConfig.StationVariant > 0 THEN
	        
	        // hold gate conveyors after button press if no maintenance is performed
	        IF #sx_GateIsDown111Filtered AND #sR_TRIG_B112.Q AND NOT #sx_MaintenanceIsPerformed THEN
	            #sx_ExternalRequestHoldGate := TRUE;
	            #sx_MaintenanceIsPerformed := TRUE;
	            
	            
	            // release gate conveyors if gate is down maintenance is performed after button press
	        ELSIF #sx_GateIsDown111Filtered AND #sR_TRIG_B111.Q AND #sx_MaintenanceIsPerformed THEN
	            #sx_ExternalRequestHoldGate := FALSE;
	            #sx_MaintenanceIsPerformed := FALSE;
	            
	            // Hold conveyor if there is no maintenance perfomed and gate has been opened
	        ELSIF NOT #sx_GateIsDown111Filtered AND NOT #sx_MaintenanceIsPerformed THEN
	            #sx_ExternalRequestHoldGate := TRUE;
	        END_IF;
	        
	        
	        // Indicate that Gate is down and stopped
	        IF #sx_MaintenanceIsPerformed AND #sx_GateIsDown111Filtered THEN
	            #qx_Light := TRUE;
	            #sx_HornHold := FALSE;
	            
	            //SCADA Signals
	            #iqUDT_WorkstationStatus."120".LiftGate.Closed := TRUE;
	            #iqUDT_WorkstationStatus."120".LiftGate.Open := FALSE;
	            
	            
	        ELSIF NOT #sx_MaintenanceIsPerformed AND #sx_GateIsDown111Filtered THEN
	            #qx_Light := FALSE;
	            #qx_Horn := FALSE;
	            #sx_HornHold := FALSE;
	            
	        ELSIF NOT #sx_GateIsDown111Filtered AND NOT #sx_GateTimeout THEN
	            #qx_Light := "DB_Memory"."Clock_0.5Hz";
	            
	            IF NOT #sx_HornHold THEN
	                #sx_Horn := TRUE;
	                
	                IF #sx_Horn THEN
	                    #sudi_HornTime += "DB_Memory".PrevCycleTime;
	                    IF #sudi_HornTime > 5000 THEN
	                        #sx_HornHold := TRUE;
	                    END_IF;
	                END_IF;
	                
	            ELSIF #sx_HornHold THEN
	                #sx_Horn := FALSE;
	                #sudi_HornTime := 0;
	            END_IF;
	            #qx_Horn := #sx_Horn;
	            
	            //SCADA Signals      
	            #iqUDT_WorkstationStatus."120".LiftGate.Closed := FALSE;
	            #iqUDT_WorkstationStatus."120".LiftGate.Open := TRUE;
	            
	            
	            
	        ELSIF #sx_GateTimeout THEN
