      sx_Hold180 { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Bool;   // Hold 180 conveyor active
      sx_Hold220 { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Bool;   // Hold 220 conveyor active
      sx_Hold230 { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Bool;   // Hold 230 conveyor active
      si_LastGIN010 { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Int;   // GIN into which data on the first buffer will be written into
      si_LastGIN110 { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Int;   // GIN into which data on the second buffer will be written into
      sx_ReleaseBinHold0X0 { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Bool;   // Hold signal for conveyors when a parcel is released from first station
      sx_ReleaseBinHold1X0 { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Bool;   // Hold signal for conveyors when a parcel is released from second station
      si_BinReleaseDistance0X0 { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Int;   // Release distance for 080 rollers
      si_BinReleaseDistance1X0 { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Int;   // Release distance for 180 rollers
      sx_BinRelease0X0 { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Bool;   // Release signal for first station
      sx_BinRelease1X0 { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Bool;   // Release signal for second station
      sx_LastRelease0X0 { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Bool;   // Variable to keep track if last release was from the first station
      sx_Clear050 { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Bool;   // Clear request for 050 tracking array
      sx_Clear060 { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Bool;   // Clear request for 060 tracking array
      sx_Clear070 { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Bool;   // Clear request for 070 tracking array
      sx_Clear150 { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Bool;   // Clear request for 160 tracking array
      sx_Clear160 { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Bool;   // Clear request for 160 tracking array
      sx_Clear170 { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Bool;   // Clear request for 170 tracking array
      sx_FirstStationActive { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Bool;   // First station is activated
      sx_SecondStationActive { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Bool;   // Second station is activated
      sFB_Pulse_UpdateBufferState { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'; S7_SetPoint := 'False'} : "FB_Pulse";   // Pulse generation for buffer state updates
      sFB_TimerOnOffDelay_0X0PushTimeout { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'; S7_SetPoint := 'False'} : "FB_TimerOnOffDelay";   // Pushout timeout for first station
      sFB_TimerOnOffDelay_1X0PushTimeout { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'; S7_SetPoint := 'False'} : "FB_TimerOnOffDelay";   // Pushout timeout for second station
      sFB_CountActiveInputs { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'; S7_SetPoint := 'False'} : "FB_CountActiveInputs";   // Function for counting occupied spaces on buffers
      sx_Hold160 { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Bool;   // Hold conveyor 160
      sx_Hold170 { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Bool;   // Hold conveyor 170
      sx_Hold150 { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Bool;   // Hold conveyor 160
      sx_Hold060 { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Bool;   // Hold conveyor 160
      sx_Hold070 { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Bool;   // Hold conveyor 160
      sx_Hold050 { ExternalAccessible := 'False'; ExternalVisible := 'False'; ExternalWritable := 'False'} : Bool;   // Hold conveyor 160
   END_VAR

   VAR_TEMP 
      tUDT_DummyBladeStop : "UDT_ValveInterface";   // Dummy Valve interface for blade stop
      ts_Barcode : String;   // Auxiliary string for copying barcodes at GIN range change
      tx_ArraySize : Int;   // Auxiliary variable for holding the size of tracking arrays
      ta_DestinationArray : Array[1..5] of Int;   // Possible destinations
      tudt_FifoStatus : "UDT_FifoStatus";   // Fifo status dummy
      tx_Dummy : Bool;   // Dummy bool
      ti_GIN : Int;   // Temporary GIN buffer
   END_VAR

   VAR CONSTANT 
      cudi_UpdateBufferState : UDInt := 5000;   // Buffer state update interval [ms]
      cs_MissingBarcodeCode : String := ' ';   // Empty string
      cudi_PushTimeout : UDInt := 20000;   // PushTimeout [ms]
      ci_DestinationLeft1 : Int := 1;
      ci_DestinationRight1 : Int := 2;
      ci_DestinationLeft2 : Int := 3;
      ci_DestinationRight2 : Int := 4;
      ci_DestinationStraight : Int := -1;
   END_VAR


BEGIN
	(*
	----------------------------------------------------------------------------------------
	                                                                                  
	Version history:
	Date        | Version   | Author        | Remark
	----------------------------------------------------------------------------------------
	12/02/2024  | 4.0       | K.Pokorski    | First Release TIA 17.5
