	20/03/2025  | 4.11      | K. Pokorski   | Station behaviour changes
	02/04/2025  | 4.12      | S. Nieswiec   | Fixed GIN ranges
	*)
	
	(*
	
	                 | First workstation |               |  Empty Bin buffer  |                      |  Empty Bin buffer  |              | First workstation |                                                            
	                +---------------------------------------------------------+ |      M           |+---------------------------------------------------------+ |    
	                |  +---+   +---+   +---+ | T +---+  | +---+   +---+   +---+ |       A          |  +---+   +---+   +---+ | T +---+   | +---+   +---+   +---+ |
	                |  |070|   |060|   |050| | R |041|  | |030|   |020|   |010| |        I         |  |010|   |020|   |030| | R |041|   | |050|   |060|   |070| |
	                |  +---+   +---+   +---+ | A +---+  | +---+   +---+   +---+ |         N        |  +---+   +---+   +---+ | A +---+   | +---+   +---+   +---+ |
	                   |-------|080|-------| | N       Z|                                            |-------|080|-------|  | N       Z |
	                |  +---+   +---+   +---+ | S +---+ O| +---+   +---+   +---+ |      L           |  +---+   +---+   +---+ | S +---+ O | +---+   +---+   +---+ |
	                |  |210|   |220|   |230| | I |241| N| |250|   |260|   |270| |       I          |  |270|   |260|   |250| | I |241| N | |230|   |220|   |210| | 
	                |  +---+   +---+   +---+ | T +---+ E| +---+   +---+   +---+ |        N         |  +---+   +---+   +---+ | T +---+ E | +---+   +---+   +---+ |
	                   |-------|180|-------| | I        |                                 E          |-------|180|-------|  | I         |                             
	                |  +---+   +---+   +---+ | O +---+  | +---+   +---+   +---+ |                  |  +---+   +---+   +---+ | O +---+   | +---+   +---+   +---+ |
	                |  |170|   |160|   |150| | N |141|  | |130|   |120|   |110| |         |        |  |110|   |120|   |130| | N |141|   | |150|   |160|   |170| |
	                |  +---+   +---+   +---+ |   +---+  | +---+   +---+   +---+ |         |        |  +---+   +---+   +---+ |   +---+   | +---+   +---+   +---+ |
	                +---------------------------------------------------------+ |         |        | +--------------------------------------------------------+ | 
	                 | Second workstation |            |Partially full Bin buffer|   \  Flow  /     |Partially full Bin buffer|          | Second workstation |            
	                                                                                  \ dir. /                                                                    
	                                                                                   \    /                                                                     
	                                                                                    \  /                                                                      
	                                                                                     \/      
	*)
	
	REGION Station flow control
	    
	    REGION Activation left station
	        
	        IF #iqUDT_FirstWESInterface.WES2PLC.On THEN
	            
	            #sx_FirstStationActive := TRUE;
	            #iqUDT_FirstWESInterface.PLC2WES.On := TRUE;
	            #iqUDT_FirstWESInterface.WES2PLC.On := FALSE;
	            
	        END_IF;
	        
	        IF #iqUDT_FirstWESInterface.WES2PLC.Off THEN
	            
	            #sx_FirstStationActive := FALSE;
	            #iqUDT_FirstWESInterface.PLC2WES.Off := TRUE;
	            #iqUDT_FirstWESInterface.WES2PLC.Off := FALSE;
	        END_IF;
	        
	        
	    END_REGION
	    
	    REGION Activation right station
	        
	        IF #iqUDT_SecondWESInterface.WES2PLC.On THEN
	            
	            #sx_SecondStationActive := TRUE;
	            #iqUDT_SecondWESInterface.PLC2WES.On := TRUE;
	            #iqUDT_SecondWESInterface.WES2PLC.On := FALSE;
	            
	        END_IF;
	        
	        IF #iqUDT_SecondWESInterface.WES2PLC.Off THEN
	            
	            #sx_SecondStationActive := FALSE;
	            #iqUDT_SecondWESInterface.PLC2WES.Off := TRUE;
	            #iqUDT_SecondWESInterface.WES2PLC.Off := FALSE;
	        END_IF;
	        
	        
	    END_REGION
	    
	    REGION Station ativation status
	        
	        
	        IF #sx_FirstStationActive XOR #sx_SecondStationActive
	        THEN
	            #qi_NrActiveStation := 1;
	        ELSIF #sx_FirstStationActive AND #sx_SecondStationActive THEN
	            #qi_NrActiveStation := 2;
	        ELSE
	            #qi_NrActiveStation := 0;
	        END_IF;
	        
	        
	    END_REGION
	    
	    REGION Timeout
	        
	        #sFB_TimerOnOffDelay_OperationTimeout(iudi_OnDelayTime := #iqUDT_Configuration.GlobalConfig.OperationTimeout,
	                                              ix_SignalToDelay := NOT #sx_NoOperationInProgress
	                                              AND #iqUDT_WorkstationCommonStatus."030".AutoON
	                                              AND #iqUDT_WorkstationCommonStatus."130".AutoON
	                                              AND #iqUDT_WorkstationCommonStatus."230".AutoON
	                                              AND #iqUDT_WorkstationCommonStatus."040".AutoON
	                                              AND #iqUDT_WorkstationCommonStatus."041".AutoON
	                                              AND #iqUDT_WorkstationCommonStatus."140".AutoON
	                                              AND #iqUDT_WorkstationCommonStatus."141".AutoON
	                                              AND #iqUDT_WorkstationCommonStatus."240".AutoON
	                                              AND #iqUDT_WorkstationCommonStatus."241".AutoON
	                                              AND (#iqUDT_WorkstationCommonStatus."050".AutoON OR #iqUDT_Configuration.GlobalConfig.StationVariant = 3 OR #iqUDT_Configuration.GlobalConfig.StationVariant = 4)
	                                              AND (#iqUDT_WorkstationCommonStatus."150".AutoON OR #iqUDT_Configuration.GlobalConfig.StationVariant = 5 OR #iqUDT_Configuration.GlobalConfig.StationVariant = 6)
	                                              AND #iqUDT_WorkstationCommonStatus."250".AutoON);
	        
	    END_REGION
	    
	    REGION Request bin to first workstation from Empty/Partial buffers 
	        // If new request from outside is received incerase counter. Counter is stored until parcel is send to proper destination. After inccreasing counter, reset request. 
	        
	        IF #iqUDT_FirstWESInterface.Field[1].WES2PLC.FIFO[0] = 'E'
	        THEN
	            #si_EmptyBinRequiredCounter0X0 += 1;
	            #iqUDT_FirstWESInterface.Field[1].WES2PLC.FIFO[0] := ' ';
	            
	        ELSIF #iqUDT_FirstWESInterface.Field[1].WES2PLC.FIFO[0] = 'P'
	        THEN
	            
	            #si_PartialyBinRequiredCounter0X0 += 1;
	            #iqUDT_FirstWESInterface.Field[1].WES2PLC.FIFO[0] := ' ';
	            
	        ELSIF #iqUDT_FirstWESInterface.Field[1].WES2PLC.FIFO[0] = 'A'
	        THEN
	            
	            IF NOT #iqUDT_WorkstationStatus."130".PEC_EoS.PEC
	            THEN
	                
	                #si_PartialyBinRequiredCounter0X0 += 1;
	                #iqUDT_FirstWESInterface.Field[1].WES2PLC.FIFO[0] := ' ';
	                
	            ELSE
	                
	                #si_EmptyBinRequiredCounter0X0 += 1;
	                #iqUDT_FirstWESInterface.Field[1].WES2PLC.FIFO[0] := ' ';
	            END_IF;
	            
	            
	        END_IF;
	    END_REGION
	    
	    REGION Request bin to second workstation from Empty/Partial buffers 
	        IF #iqUDT_SecondWESInterface.Field[1].WES2PLC.FIFO[0] = 'E'
	        THEN
	            #si_EmptyBinRequiredCounter1X0 += 1;
	            #iqUDT_SecondWESInterface.Field[1].WES2PLC.FIFO[0] := ' ';
	            
	        ELSIF #iqUDT_SecondWESInterface.Field[1].WES2PLC.FIFO[0] = 'P'
	        THEN
	            
	            #si_PartialyBinRequiredCounter1X0 += 1;
	            #iqUDT_SecondWESInterface.Field[1].WES2PLC.FIFO[0] := ' ';
	            
	        ELSIF #iqUDT_SecondWESInterface.Field[1].WES2PLC.FIFO[0] = 'A'
	        THEN
	            
	            IF NOT #iqUDT_WorkstationStatus."130".PEC_EoS.PEC
	            THEN
	                
	                #si_PartialyBinRequiredCounter1X0 += 1;
	                #iqUDT_SecondWESInterface.Field[1].WES2PLC.FIFO[0] := ' ';
	                
	            ELSE
	                
	                #si_EmptyBinRequiredCounter1X0 += 1;
	                #iqUDT_SecondWESInterface.Field[1].WES2PLC.FIFO[0] := ' ';
	            END_IF;
	            
	            
	        END_IF;
	        
	    END_REGION
	    
	    REGION Send Bin from 0X0
	        
	        // When no operation is proceed check if any operation is required. If yes then send bin with destination and start sending procedure. 
	        
	        IF #sx_NoOperationInProgress AND NOT #iqUDT_WorkstationStatus."030".PEC_EoS.PEC THEN
	            
	            IF (#iqUDT_WorkstationStatus."050".PEC_EoS.PEC OR #iqUDT_WorkstationStatus."060".PEC_EoS.PEC OR #iqUDT_WorkstationStatus."070".PEC_EoS.PEC)
	                AND #si_EmptyBinRequiredCounter0X0 >= 1
	                AND #sx_FirstStationActive
	                AND (#iqUDT_Configuration.GlobalConfig.StationVariant = 1 OR #iqUDT_Configuration.GlobalConfig.StationVariant = 2 OR #iqUDT_Configuration.GlobalConfig.StationVariant = 5 OR #iqUDT_Configuration.GlobalConfig.StationVariant = 6)
	            THEN
	                
	                #sx_Enable0X0In := TRUE;
	                
	                #si_040DestinationA := 2;
	                #si_040DestinationB := 0;
	                #si_040DestinationC := 0;
	                #si_040DestinationD := 0;
	                
	                #si_140DestinationA := 0;
	                #si_140DestinationB := 0;
	                #si_140DestinationC := 0;
	                #si_140DestinationD := 0;
	                
	                #si_240DestinationA := 0;
	                #si_240DestinationB := 0;
	                #si_240DestinationC := 0;
	                #si_240DestinationD := 0;
	                
	                #sx_Hold030 := FALSE;
	                #sx_Hold130 := TRUE;
	                #sx_Hold020 := TRUE;
	                
	                #sx_Hold050 := FALSE;
	                IF NOT #iqUDT_WorkstationStatus."050".PEC_EoS.PEC THEN
	                    #sx_Hold060 := FALSE;
	                END_IF;
	                IF NOT #iqUDT_WorkstationStatus."050".PEC_EoS.PEC AND NOT #iqUDT_WorkstationStatus."060".PEC_EoS.PEC THEN
	                    #sx_Hold070 := FALSE;
	                END_IF;
	                
	                
	                
	                #sx_NoOperationInProgress := FALSE;
	                #si_EmptyBinRequiredCounter0X0 -= 1;
	                #si_SendTo0X0 := TRUE;
	                
	                
	                
	            ELSIF (#iqUDT_WorkstationStatus."150".PEC_EoS.PEC OR #iqUDT_WorkstationStatus."160".PEC_EoS.PEC OR #iqUDT_WorkstationStatus."170".PEC_EoS.PEC) AND #si_EmptyBinRequiredCounter1X0 >= 1 AND #sx_SecondStationActive
	            THEN
	                
	                #sx_Enable0X0In := TRUE;
	                IF #iqUDT_Configuration.GlobalConfig.StationVariant = 2 OR #iqUDT_Configuration.GlobalConfig.StationVariant = 4
	                THEN
	                    #si_040DestinationA := 4;
	                    #si_040DestinationB := 0;
	                    #si_040DestinationC := 0;
	                    #si_040DestinationD := 0;
	                    
	                    #si_140DestinationA := 0;
	                    #si_140DestinationB := 0;
	                    #si_140DestinationC := 2;
	                    #si_140DestinationD := 0;
	                    
	                    #si_240DestinationA := 0;
	                    #si_240DestinationB := 0;
	                    #si_240DestinationC := 0;
	                    #si_240DestinationD := 3;
	                ELSIF #iqUDT_Configuration.GlobalConfig.StationVariant = 1 OR #iqUDT_Configuration.GlobalConfig.StationVariant = 3
	                THEN
	                    #si_040DestinationA := 3;
	                    #si_040DestinationB := 0;
	                    #si_040DestinationC := 0;
	                    #si_040DestinationD := 0;
	                    
	                    #si_140DestinationA := 0;
	                    #si_140DestinationB := 0;
	                    #si_140DestinationC := 0;
	                    #si_140DestinationD := 2;
	                    
	                    #si_240DestinationA := 0;
	                    #si_240DestinationB := 0;
	                    #si_240DestinationC := 4;
	                    #si_240DestinationD := 0;
	                END_IF;
	                
	                #sx_Hold030 := FALSE;
	                #sx_Hold130 := TRUE;
	                #sx_Hold020 := TRUE;
	                
	                #sx_Hold150 := FALSE;
	                IF NOT #iqUDT_WorkstationStatus."150".PEC_EoS.PEC THEN
	                    #sx_Hold160 := FALSE;
	                END_IF;
	                IF NOT #iqUDT_WorkstationStatus."150".PEC_EoS.PEC AND NOT #iqUDT_WorkstationStatus."160".PEC_EoS.PEC THEN
	                    #sx_Hold170 := FALSE;
	                END_IF;
	                
	                
	                
	                #sx_NoOperationInProgress := FALSE;
	                #si_EmptyBinRequiredCounter1X0 -= 1;
	                #si_SendTo1X0 := True;
	                
	            END_IF;
	            
	        END_IF;
	        
	    END_REGION
	    
	    REGION Send Bin from 1X0
	        
	        // When no operation is proceed check if any operation is required. If yes then send bin with destination and start sending procedure. 
	        IF #sx_NoOperationInProgress AND NOT #iqUDT_WorkstationStatus."130".PEC_EoS.PEC
	        THEN
	            
	            IF (#iqUDT_WorkstationStatus."050".PEC_EoS.PEC OR #iqUDT_WorkstationStatus."060".PEC_EoS.PEC OR #iqUDT_WorkstationStatus."070".PEC_EoS.PEC)
	                AND #si_PartialyBinRequiredCounter0X0 >= 1 AND #sx_FirstStationActive
	            THEN
	                
	                #sx_Enable1X0In := TRUE;
	                IF #iqUDT_Configuration.GlobalConfig.StationVariant = 2 OR #iqUDT_Configuration.GlobalConfig.StationVariant = 6
	                THEN
	                    #si_040DestinationA := 0;
	                    #si_040DestinationB := 0;
	                    #si_040DestinationC := 0;
	                    #si_040DestinationD := 2;
	                    
	                    #si_140DestinationA := 3;
	                    #si_140DestinationB := 0;
	                    #si_140DestinationC := 0;
	                    #si_140DestinationD := 0;
	                    
	                    #si_240DestinationA := 0;
	                    #si_240DestinationB := 0;
	                    #si_240DestinationC := 4;
	                    #si_240DestinationD := 0;
	                ELSIF #iqUDT_Configuration.GlobalConfig.StationVariant = 1 OR #iqUDT_Configuration.GlobalConfig.StationVariant = 5
	                THEN
	                    #si_040DestinationA := 0;
	                    #si_040DestinationB := 0;
	                    #si_040DestinationC := 2;
	                    #si_040DestinationD := 0;
	                    
	                    #si_140DestinationA := 4;
	                    #si_140DestinationB := 0;
	                    #si_140DestinationC := 0;
	                    #si_140DestinationD := 0;
	                    
	                    #si_240DestinationA := 0;
	                    #si_240DestinationB := 0;
	                    #si_240DestinationC := 0;
	                    #si_240DestinationD := 3;
	                END_IF;
	                
	                #sx_Hold130 := FALSE;
	                #sx_Hold030 := TRUE;
	                #sx_Hold120 := TRUE;
	                
	                #sx_Hold050 := FALSE;
	                IF NOT #iqUDT_WorkstationStatus."050".PEC_EoS.PEC THEN
	                    #sx_Hold060 := FALSE;
	                END_IF;
	                IF NOT #iqUDT_WorkstationStatus."050".PEC_EoS.PEC AND NOT #iqUDT_WorkstationStatus."060".PEC_EoS.PEC THEN
	                    #sx_Hold070 := FALSE;
	                END_IF;
	                
	                
	                #sx_NoOperationInProgress := FALSE;
	                #si_PartialyBinRequiredCounter0X0 -= 1;
	                #si_SendTo0X0 := TRUE;
	                
	                
	            ELSIF (#iqUDT_WorkstationStatus."150".PEC_EoS.PEC OR #iqUDT_WorkstationStatus."160".PEC_EoS.PEC OR #iqUDT_WorkstationStatus."170".PEC_EoS.PEC)
	                AND #si_PartialyBinRequiredCounter1X0 >= 1
	                AND #sx_SecondStationActive
	                AND (#iqUDT_Configuration.GlobalConfig.StationVariant = 1 OR #iqUDT_Configuration.GlobalConfig.StationVariant = 2 OR #iqUDT_Configuration.GlobalConfig.StationVariant = 3 OR #iqUDT_Configuration.GlobalConfig.StationVariant = 4)
	            THEN
	                
	                #sx_Enable1X0In := TRUE;
	                #si_040DestinationA := 0;
	                #si_040DestinationB := 0;
	                #si_040DestinationC := 0;
	                #si_040DestinationD := 0;
	                
	                #si_140DestinationA := 2;
	                #si_140DestinationB := 0;
	                #si_140DestinationC := 0;
	                #si_140DestinationD := 0;
	                
	                #si_240DestinationA := 0;
	                #si_240DestinationB := 0;
	                #si_240DestinationC := 0;
	                #si_240DestinationD := 0;
	                
	                
	                #sx_Hold130 := FALSE;
	                #sx_Hold030 := TRUE;
	                #sx_Hold120 := TRUE;
	                
	                #sx_Hold150 := FALSE;
	                IF NOT #iqUDT_WorkstationStatus."150".PEC_EoS.PEC THEN
	                    #sx_Hold160 := FALSE;
	                END_IF;
	                IF NOT #iqUDT_WorkstationStatus."150".PEC_EoS.PEC AND NOT #iqUDT_WorkstationStatus."160".PEC_EoS.PEC THEN
	                    #sx_Hold170 := FALSE;
	                END_IF;
	                
	                
	                #sx_NoOperationInProgress := FALSE;
	                #si_PartialyBinRequiredCounter1X0 -= 1;
	                #si_SendTo1X0 := TRUE;
	                
	            END_IF;
	            
	        END_IF;
	        
	        
	        
	    END_REGION
	    
	    REGION Send Bin from 2X0
	        
	        // When no operation is proceed check if any operation is required. If yes then send bin with destination and start sending procedure. 
	        
	        IF #sx_NoOperationInProgress AND NOT #iqUDT_WorkstationStatus."230".PEC_EoS.PEC AND #iqUDT_WorkstationStatus."250".PEC_EoS.PEC THEN
	            
	            
	            #sx_Enable2X0In := TRUE;
	            
	            #si_040DestinationA := 0;
	            #si_040DestinationB := 0;
	            #si_040DestinationC := 0;
	            #si_040DestinationD := 0;
	            
	            #si_140DestinationA := 0;
	            #si_140DestinationB := 0;
	            #si_140DestinationC := 0;
	            #si_140DestinationD := 0;
	            
	            #si_240DestinationA := 2;
	            #si_240DestinationB := 0;
	            #si_240DestinationC := 0;
	            #si_240DestinationD := 0;
	            
	            
	            #sx_Hold230 := FALSE;
	            #sx_Hold220 := TRUE;
	            
	            
	            #sx_NoOperationInProgress := FALSE;
	            #si_SendTo2X0 := TRUE;
	            
	        END_IF;
	        
	    END_REGION
	    
	    REGION Buffer clearing sequence when both workstations are deativated
	        
	        IF #sx_NoOperationInProgress AND #iqUDT_WorkstationStatus."250".PEC_EoS.PEC AND NOT #sx_FirstStationActive AND NOT #sx_SecondStationActive
	        THEN
	            
	            IF NOT #iqUDT_WorkstationStatus."230".PEC_EoS.PEC
	            THEN
	                
	                #sx_Enable2X0In := TRUE;
	                
