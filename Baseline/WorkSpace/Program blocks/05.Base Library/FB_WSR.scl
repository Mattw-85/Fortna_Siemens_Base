	            #si_LastGIN110 += 1;
	            
	            IF #si_LastGIN110 < ((#iqUDT_Configuration.GlobalConfig.GINRangeStart + #iqUDT_Configuration.GlobalConfig.GINRangeEnd) / 2) + 1
	                OR #si_LastGIN110 > #iqUDT_Configuration.GlobalConfig.GINRangeEnd
	            THEN
	                #si_LastGIN110 := ((#iqUDT_Configuration.GlobalConfig.GINRangeStart + #iqUDT_Configuration.GlobalConfig.GINRangeEnd) / 2) +1;
	            END_IF;
	            "DB_GIN".GIN[#si_LastGIN110] := "DB_GIN".GIN[#iqUDT_HandshakeUpstream110.GIN];
	            //     #ts_Barcode := "DB_GIN".GIN[#iqUDT_HandshakeUpstream110.GIN].General.Identification.Barcode;
	            //     #ti_DivertDestination := "DB_GIN".GIN[#iqUDT_HandshakeUpstream110.GIN].General.SortStatus.DivertDestination;
	            #iqUDT_HandshakeUpstream110.GIN := #si_LastGIN110;
	            
	            IF "DB_GIN".GIN[#iqUDT_HandshakeUpstream110.GIN].General.SortStatus.GoodDivert
	            THEN
	                "DB_GIN".GIN[#iqUDT_HandshakeUpstream110.GIN].WCS.ReasonCode := '1';
